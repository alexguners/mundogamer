                <div class="panel-body">
                    <form id="LoginForm" role="form" class="form-horizontal" enctype="multipart/form-data" autocomplete="off"  method="post">
                       <div class="col-lg-6">
                          <!-- Campo Nome do Parceiro -->
                         <p><label>Nome do Parceiro&nbsp;</label>
                         <input type="text"class="form-control"id="nome_parceiro" name="nome_parceiro"placeholder="Nome do parceiro"required="required"/></p>
                         <!-- Fim do Campo Nome do Parceiro -->

                         <!-- Campo Nome do Parceiro -->
                         <p><label>E-mail de contato&nbsp;</label>
                         <input type="text" class="form-control" id="email_contato" name="email_contato" placeholder="Digite o e-mail de contato" required="required" /></p>
                         <!-- Fim do Campo Nome do Parceiro --> 

                         <!-- Campo Estado do Parceiro -->
                         <p><label>Estado&nbsp;</label>
                         <select  name="herolist" id="estado_parceiro" class="select-block" required="required"></select></p>
                         <!-- Fim do Estado Nome do Parceiro -->  

                         <!-- Campo Cidade do Parceiro -->
                         <p><label>Cidade&nbsp;</label>
                         <select  name="herolist" id="cidade_parceiro" class="select-block" required="required"></select></p>
                         <!-- Fim do Cidade Nome do Parceiro --> 

                         <!-- categoria da notícia-->
                         <p><label>Categoria:</label>
                         <select  name="herolist" id="categoria" class="select-block" required="required">
                                 <option value="0">Selecione uma Categoria</option>
                                 <option value="Blog/Site">Blog/Site</option>          
                                 <option value="Loja">Loja</option>
                                 <option value="Página no Facebook">Página Facebook</option>
                                 <option value="Outros">Outros</option>
                         </select></p>                         
                         <!-- Fim da categoria da notícia -->
