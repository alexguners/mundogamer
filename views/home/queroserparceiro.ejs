                           <input type="text" class="form-control" id="site " name="site" placeholder="http://meusite.com"/></p>
                           <!-- Fim do Campo Site do Parceiro -->

                           <!-- Campo Twitter do Parceiro -->
                           <p><label>Perfil Twitter</label>
                           <input type="text"  placeholder="Endereço Twitter" id="twitter" name="twitter" class="form-control"/></p>
                           <!-- Fim Campo Twitter do Parceiro -->

                           <!-- Campo Facebook do Parceiro -->
                           <p><label>Página Facebook</label>
                           <input type="text" name="facebook" id="facebook" class="form-control" placeholder="http://www.facebook.com/minhapagina"/></p>
                           <!-- FIM do Campo Facebook do Parceiro -->

