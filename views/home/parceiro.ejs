            <div class="table-responsive">
            <center><img class="media-object" width="270" src="/images/wikigamer/parceiros/logos/<%= parceiro._id %>" ></center>
                <table class="table">

                 <tbody background-color="#808080">
                    <% if (typeof parceiro.nome_parceiro !== 'undefined'){ %>
                    <% if (parceiro.nome_parceiro !== 'undefined'){ %> 
                      <tr>
                         <td><span class="label label-primary">Nome</span></td>
                         <td align="center"><small><%= parceiro.nome_parceiro %></small></td>
                      </tr>
                      <%} if ( (parceiro.categoria !== 'undefined') && (parceiro.categoria !== '') ) { %> 
                      <tr>
                       <td><span class="label label-primary">Categoria</span></td>
                       <td align="center"><span class="label label-warning"><%= parceiro.categoria %></span></td>
                      </tr>
                      <%} if ( (parceiro.email_contato !== 'undefined') && (parceiro.email_contato !== '') ) { %> 
                      <tr>
                       <td><span class="label label-primary">Email</span></td>
                       <td align="center"><a id="email_contato" rel="popover"  data-placement="top" href="mailto:<%- parceiro.email_contato %>" target="_top"><span class="label label-default">Enviar email</span></a></td>
                      </tr>
                      <%} if ( (parceiro.facebook !== 'undefined') && (parceiro.facebook !== '') ) { %>
                      <tr>
                         <td><a href="https://facebook.com/<%= parceiro.facebook %>" target="_blank"><span class="label label-primary">Facebook</span></a></td>
                         <td align="center"><div class="fb-like" data-href="https://facebook.com/<%- parceiro.facebook %>" data-layout="button_count" data-action="like" data-show-faces="false" data-share="false"></div></td>
                      </tr>
                      <%} if ( (parceiro.instagram !== 'undefined') && (parceiro.instagram !== '') ) { %>
                      <tr>
                         <td><span class="label label-primary">Instagram</span></td>
                         <td align="center"><a href="<%- parceiro.instagram %>"><a href="http://instagram.com/<%- parceiro.instagram %>?ref=badge" class="ig-b- ig-b-v-24" target="_blank"><img src="//badges.instagram.com/static/images/ig-badge-view-24.png" alt="Instagram"  /></a></td>
                      </tr>
                      <%} if ( (parceiro.twitter !== 'undefined') && (parceiro.twitter !== '') ) { %>
                      <tr>
                         <td><a href="https://twitter.com/<%- parceiro.twitter %>"><span class="label label-primary">Twitter</span></a></td>
                         <td align="center"><a href="https://twitter.com/<%- parceiro.twitter %>" class="twitter-follow-button" data-show-count="false">Seguir @<%- parceiro.twitter %></a></td>
                      </tr>
                      <%} if ( (parceiro.canal_youtube !== 'undefined') && (parceiro.canal_youtube !== '') ) { %>
                      <tr>
                         <td><a href="https://youtube.com/<%- parceiro.canal_youtube %>" target="_blank"><span class="label label-primary">Youtube</span></td>
                         <td align="center"><div class="g-ytsubscribe" data-channel="<%- parceiro.canal_youtube %>" data-layout="default" data-count="default"></div></td>
                      </tr>
                      <%} if ( (parceiro.site !== 'undefined') && (parceiro.site !== '') ) { %>
                      <tr>
                         <td><span class="label label-primary">Site Oficial</span></td>
                         <td align="center"><a href="<%- parceiro.site %>" target="_blank"><span class="label label-success">Acessar site</span></a></td>
                      </tr>
                      <%}%> 
                      <tr>
                       <td class="success"><small>Visualizações</small></td>
                       <td class="success"><small><% if (typeof parceiro.views == 'undefined') {%>1 visualização<% }else{ %><%= parceiro.views %> visualizações<%}%></small></a></small></a></td>
                      </tr>
                    <%}%>

                 </tbody>
              </table>
