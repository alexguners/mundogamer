            <!-- Titulo da noticias -->
            <div><center><h4><%= noticia.titulo %></h4></center></div>
            <!-- /. Titulo da noticias -->

            <!-- descrição do autor e botoes compartilha --> 
            <div class="typography-row"> 
              <dl class="palette palette-midnight-blue">
                  <dt>Postado por <%=noticia._criador.local.nome%>
                   <div style="float:right;margin-top:-2px;margin-right:100px"><quote>Compartilhe essa notícia em suas redes sociais</quote></div>
                    <dd> <% var d = new Date(noticia.data); 
                        %>no dia <%= ('0' + d.getDate()).slice(-2) + '/' + ('0' + (d.getMonth()+1)).slice(-2) + '/'  + d.getFullYear() %> às <%= ('0' + (d.getHours())).slice(-2) + ':' + ('0' + (d.getMinutes())).slice(-2) %>
                        <!-- Essa notícia já foi acessada <%- noticia.views%> vezes -->

                        <!-- Google + -->
                        <div style="float:right;" >
                          <a  class="g-plus"  data-action="share" data-annotation="bubble"></a> 
                          <!-- Botão do Twitter -->
                          <a href="https://twitter.com/share" class="twitter-share-button" data-hashtags="MundoGamer" data-text="<%= noticia.titulo %>" data-via="brasilmundogame" data-lang="pt">Tweetar</a>
                        </div>
                        <!-- Facebook like -->
                        <div style="float:right;padding: 2px; margin-top:-2px;">
                          <a class="fb-like" data-href="http://mundogamer.com.br/noticia/<%= noticia._id %>" data-layout="button_count" data-action="like" data-show-faces="true" data-share="true"></a><!-- Botão do Facebook -->
                        </div><!-- /.Facebook like -->
                    </dd>
                  </dt>
            </dl>
          </div><!-- /. descrição do autor e botoes comparilha -->

          <!-- descrição da noticia -->
          <div><p><%- noticia.descricao %></p></div> <!-- /.descrição da noticia -->
          <!-- /.descrição da noticia -->

          <% if (typeof noticia.nome_fonte !== 'undefined'){ %>
